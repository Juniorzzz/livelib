<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.unityandroidlive">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:name="com.live.livelib.Scheduler">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.live.livelib.LiveActivity"
            android:theme="@android:style/Theme.Translucent.NoTitleBar" />

        <service android:name="com.live.livelib.LiveService" android:process=":remote" android:enabled="true" android:exported="true">
            <intent-filter>
                <action android:name="com.live.livelib.LiveService"/>
            </intent-filter>
        </service>
    </application>

</manifest>